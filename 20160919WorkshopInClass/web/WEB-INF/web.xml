<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>com.billzhonggz.workshop2.HelloWorld</servlet-class>
        <servlet-name>HelloPara</servlet-name>
        <servlet-class>com.billzhonggz.workshop2.HelloPara</servlet-class>
        <servlet-name>HelloPost</servlet-name>
        <servlet-class>com.billzhonggz.workshop2.HelloPost</servlet-class>
        <servlet-name>HelloHomework</servlet-name>
        <servlet-class>com.billzhonggz.workshop2.HelloHomework</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/hello</url-pattern>
        <servlet-name>HelloPara</servlet-name>
        <url-pattern>/hellopara</url-pattern>
        <servlet-name>HelloPost</servlet-name>
        <url-pattern>/hellopost</url-pattern>
        <servlet-name>HelloHomework</servlet-name>
        <url-pattern>/hellohomework</url-pattern>
    </servlet-mapping>
</web-app>